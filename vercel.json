{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./config(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./controllers(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./middlewares(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./models(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./routes(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./utils(.*)",
      "dest": "./index.js"
    },
    {
      "src": "./validators(.*)",
      "dest": "./index.js"
    }
  ]
}
